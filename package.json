{
  "name": "plates-for-us",
  "version": "1.0.0",
  "description": "",
  "main": "dist/bundle.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "webpack serve --mode development --open",
    "dev:electron": "wait-on http://localhost:5173 --timeout 30000 && tsc -p electron && electron ./dist/main.js",
    "dev:web": "webpack serve --mode development --open",
    "build": "webpack --mode production && electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "framer-motion": "^12.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "concurrently": "^6.0.0",
    "css-loader": "^7.1.2",
    "electron": "^22.3.27",
    "electron-builder": "^25.1.8",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.9.5",
    "wait-on": "^8.0.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "build": {
    "appId": "com.platesforus.app",
    "files": [
      "dist/**/*",
      "!**/*.map"
    ],
    "directories": {
      "output": "dist"
    }
  }
}
