{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "jsx": "react",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "noEmit": true
    },
    "include": ["src", "src/types"]
  }